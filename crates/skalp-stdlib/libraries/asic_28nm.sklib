fit = 0.05
area_um2 = 0.4
transistor_count = 2
drive_strength = 1
nominal_voltage_mv = 1000
min_voltage_mv = 700
timing_margin_voltage_mv = 850
voltage_delay_coefficient = 0.12
voltage_sensitivity = 6

[[cells.failure_modes]]
name = "stuck_at_0"
