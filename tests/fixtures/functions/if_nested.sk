// Phase 4 Test: Function with nested if expressions

fn clamp(x: bit[8], min: bit[8], max: bit[8]) -> bit[8] {
    return if x < min { min } else { if x > max { max } else { x } };
}

entity IfNestedTest {
    in clk: clock
    in x: bit[8]
    in min: bit[8]
    in max: bit[8]
    out result: bit[8]
}

impl IfNestedTest {
    result = clamp(x, min, max)
}
