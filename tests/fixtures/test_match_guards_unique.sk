entity UniqueGuards {
    in clk: clock
    in state_val: nat[2]
    in enable: bool
    out output: nat[4]
}

impl UniqueGuards {
    on(clk.rise) {
        match state_val {
            0 if enable => output <= 5,
            1 if enable => output <= 6,
            _ => output <= 0,
        }
    }
}
