// FP32 Add gate-level simulation test
use skalp::numeric::fp::*;

async entity FP32AddTest {
    in a: bit[32]
    in b: bit[32]
    out result: bit[32]
}

impl FP32AddTest {
    signal a_fp: fp32 = a as fp32
    signal b_fp: fp32 = b as fp32
    signal sum: fp32 = a_fp + b_fp
    result = sum as bit[32]
}
